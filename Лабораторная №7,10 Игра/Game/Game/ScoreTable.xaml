        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Game"
        mc:Ignorable="d"
        Title="ScoreTable" Height="300" Width="467.334" Activated="Window_Activated" WindowStartupLocation="CenterScreen">
